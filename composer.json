{
    "name": "ssplugin/ss-membership",
    "description": "Craft CMS membership with stripe.",
    "type": "craft-plugin",
    "version": "1.0.3",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "ssmembership"
    ],
    "support": {
        "docs": "http://datadazzle.com/ssplugin",
        "issues": "https://github.com/ssplugin/ss-craft-membership/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ssplugin",
            "homepage": "http://www.systemseeders.com/"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0",
        "stripe/stripe-php": "7.92.0",
        "php": "^8.0"
    },
    "require-dev": {     
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "ssplugin\\ssmembership\\": "src/"
        }
    },
    "extra": {
        "name": "SS Craft Membership",
        "handle": "ss-membership",
        "developer": "ssplugin",
        "developerUrl": "http://www.systemseeders.com/",
        "documentationUrl": "http://datadazzle.com/ssplugin",
        "changelogUrl": "https://github.com/ssplugin/ss-craft-membership/blob/main/CHANGELOG.md",
        "components": {
            "membership": "ssplugin\\ssmembership\\services\\Membership"
        },
        "class": "ssplugin\\ssmembership\\SsMembership"
    }
}
